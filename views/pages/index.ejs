            <div class="row" >
                <h4 > Selecte une Departement </h4>
             </div>
            <div class="espace"></div> 
            <form action="/desc" method="post">
            <select   class="form-control form-control-lg" name="select" id="option">
              <% if (locals.test) { %>
              <% for(var i = 0; i <test.length; i++) { %>
                <option value='<%= test[i] %>'><%= test[i] %></option>
              <% } %>
              <% } %>
            </select> <br><br><br>
              <button type="submit" class="btn btn-secondary" > ok </button>
              </form>
              
              <div class="espace"></div> 
            
              <div class="s1" >
              <h6 style="color: gray;"> Nom de département et :  </h6>
              <% if (locals.test1) { %>
              <h5 style="text-align: center;" ><%= test1.nome %></h5>
              <div class="espace"></div> 
              <h6  style="color: gray;"> Déscription de département et :   </h6><br>
              <p style="text-align: center;"><%= test1.desc %></p>
              <% } %>
              <div class="espace"></div> 
            </div>
              <div class="espace"></div> 
              
              

             

              


        </div>

        <!-- ajouter departement -->
         
        <div class="ajouter" class="form-group">
                <div class="row" >
                <h4 >Ajouter une Departement </h4>
                <button id="btn"  type="button" class="btn btn-secondary" data-toggle="tooltip" data-html="true" style="margin-left: 5%;" >
                    <div class="pl">+</div>
                  </button> 
                  
            </div>
            <% if(locals.error){ %>
              <div class="ui" style="color: red;"  ><h5 style="text-align: center;"><%= error %></h5></div>
              <% } %>
                   <% if(locals.valid){ %>
                   <div class="ui" style="color: green;" ><h5 style="text-align: center;"><%= valid %></h5></div>
                 <% } %>
                 </div>
                  
            
